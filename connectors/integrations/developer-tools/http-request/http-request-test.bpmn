    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Activity_HTTPRequest" />
    <bpmn:serviceTask id="Activity_HTTPRequest" name="Create Blog Post" camunda:modelerTemplate="io.catalyst.template.http-request" camunda:modelerTemplateVersion="1" camunda:class="io.catalyst.bridge.CatalystBridge">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="timeout">30</camunda:inputParameter>
          <camunda:inputParameter name="outputMapping">{
  "success": "$.success",
  "statusCode": "$.statusCode",
  "responseBody": "$.responseBody",
  "responseHeaders": "$.responseHeaders",
  "error": "$.error"
}</camunda:inputParameter>
          <camunda:inputParameter name="webhookUrl">http://catalyst-n8n:5678/webhook-test/catalyst-http-request</camunda:inputParameter>
          <camunda:inputParameter name="payload">{
  "method": "POST",
  "url": "https://jsonplaceholder.typicode.com/posts",
  "headers": {},
  "queryParams": {},
  "body": {
    "title": "My Post",
    "body": "Content here",
    "userId": 1
  }
}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Activity_HTTPRequest" targetRef="Gateway_CheckSuccess" />
    <bpmn:exclusiveGateway id="Gateway_CheckSuccess" name="Request successful?" default="Flow_Error">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_Success</bpmn:outgoing>
      <bpmn:outgoing>Flow_Error</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_Success" name="Yes" sourceRef="Gateway_CheckSuccess" targetRef="Activity_LogSuccess">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${success == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Error" name="No" sourceRef="Gateway_CheckSuccess" targetRef="Activity_LogError" />
    <bpmn:scriptTask id="Activity_LogSuccess" name="Log Success" scriptFormat="javascript">
      <bpmn:incoming>Flow_Success</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
      <bpmn:script>var body = execution.getVariable("responseBody");
print("HTTP Request succeeded!");
print("Status Code: " + execution.getVariable("statusCode"));
print("Created Post ID: " + body.id);
print("Response: " + JSON.stringify(body));</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:scriptTask id="Activity_LogError" name="Log Error" scriptFormat="javascript">
      <bpmn:incoming>Flow_Error</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
      <bpmn:script>print("HTTP Request failed!");
print("Status Code: " + execution.getVariable("statusCode"));
print("Error: " + execution.getVariable("error"));</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Activity_LogSuccess" targetRef="Activity_0hitxsm" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Activity_LogError" targetRef="Activity_0h9zjfr" />
    <bpmn:endEvent id="EndEvent_Success" name="Success">
      <bpmn:incoming>Flow_0gwg9fl</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_Error" name="Error">
      <bpmn:incoming>Flow_0yxyd5r</bpmn:incoming>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_1" errorRef="Error_HTTPFailed" />
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0gwg9fl" sourceRef="Activity_0hitxsm" targetRef="EndEvent_Success" />
    <bpmn:userTask id="Activity_0hitxsm" name="View successful result" camunda:assignee="demo">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_0gwg9fl</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Activity_0h9zjfr" name="View ErrorÂ  result" camunda:assignee="demo">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_0yxyd5r</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0yxyd5r" sourceRef="Activity_0h9zjfr" targetRef="EndEvent_Error" />
  </bpmn:process>
